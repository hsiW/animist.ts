{
  "name": "animist",
  "version": "0.1.1",
  "description": "",
  "keywords": [],
  "main": "dist/animist.umd.js",
  "module": "dist/animist.es5.js",
  "typings": "dist/types/animist.d.ts",
  "files": [
    "dist/types",
    "dist/animist.es5.js",
    "dist/animist.es5.js.map",
    "dist/animist.umd.js",
    "dist/animist.umd.js.map",
    "LICENSE.md",
    "README.md"
  ],
  "author": "Mei Boudreau<xanizl@me.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hsiW/animist.ts"
  },
  "license": "ISC",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && rollup -c && rimraf compiled && typedoc --out dist/docs --target es6 --theme minimal src",
    "start": "tsc-watch --onSuccess \"rollup -c\"",
    "test": "jest",
    "deploy-docs": "ts-node tools/gh-pages-publish"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^20.0.0",
    "@types/node": "^8.0.0",
    "jest": "^20.0.4",
    "lodash.camelcase": "^4.3.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.46.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "ts-jest": "^20.0.6",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "tslint-config-enkel": "https://github.com/zeyla/tslint-config-enkel",
    "typedoc": "^0.7.1",
    "typescript": "^2.3.4"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^0.16.2"
  }
}
